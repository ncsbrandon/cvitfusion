<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle fx:id="nodeBody" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="75.0" stroke="BLACK" strokeLineCap="ROUND" strokeType="INSIDE" width="150.0" />
      <MenuButton layoutX="114.0" layoutY="2.0" mnemonicParsing="false">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#onMenuAction" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </MenuButton>
      <Label fx:id="nodeName" layoutX="24.0" layoutY="14.0" text="node name" />
   </children>
</AnchorPane>
